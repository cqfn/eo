+package sandbox.hse
+alias stdout org.eolang.hse.io.stdout
+alias sprintf org.eolang.hse.txt.sprintf

[args...] > appStrarrtostdout
  strarrtostdout args > @

[arr] > strarrtostdout
  reduce. > @
    arr
    "result:"
    []
      [accum current] > reduce
        sprintf > @
          "%s %s"
          accum
          current
